[ignore]
.*/node_modules/.*
.*/build/.*
.*/dist/.*

[include]

[libs]
flow-typed

[lints]
sketchy-null=error
untyped-type-import=error
nonstrict-import=error
unclear-type=error
unsafe-getters-setters=error
unnecessary-optional-chain=error

[options]
emoji=true
exact_by_default=true
include_warnings=true
module.file_ext=.js
module.file_ext=.mjs
module.file_ext=.cjs
module.file_ext=.jsx
module.file_ext=.json
module.file_ext=.module.scss
module.file_ext=.svg
module.file_ext=.png

[strict]
nonstrict-import
unclear-type
unsafe-getters-setters
untyped-import
untyped-type-import
sketchy-null
